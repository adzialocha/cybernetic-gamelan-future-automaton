{
  "name": "cybernetic-gamelan-future-automaton",
  "version": "1.2.1",
  "description": "Network-based browser game for human laptop performers",
  "main": "automaton/index.html",
  "engines": {
    "node": "~8.9.1",
    "npm": "~5.5.1"
  },
  "scripts": {
    "build": "npm run build:docs && npm run build:play",
    "build:docs": "make-dir ./dist && static-generator build --source_dir ./docs --output_dir ./dist",
    "build:play": "npm run clean:play && cross-env NODE_ENV=production webpack",
    "clean": "npm run clean:dist && npm run clean:node",
    "clean:dist": "rimraf dist",
    "clean:node": "rimraf node_modules/",
    "clean:play": "rimraf dist/play",
    "lint": "npm run lint:js && npm run lint:scss",
    "lint:js": "eslint automaton/scripts; exit 0",
    "lint:scss": "sass-lint -c .sass-lint.yml '{automaton,docs}/**/*.scss' -v -q",
    "postinstall": "npm run build",
    "serve": "webpack-dev-server --host 0.0.0.0",
    "start": "node server/index.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/adzialocha/cybernetic-gamelan-future-automaton.git"
  },
  "author": "adzialocha",
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/adzialocha/cybernetic-gamelan-future-automaton/issues"
  },
  "private": true,
  "homepage": "https://github.com/adzialocha/cybernetic-gamelan-future-automaton#readme",
  "devDependencies": {
    "babel-core": "6.26.0",
    "babel-eslint": "^8.1.2",
    "babel-loader": "7.1.2",
    "babel-plugin-add-module-exports": "0.2.1",
    "babel-plugin-transform-object-rest-spread": "6.26.0",
    "babel-preset-env": "1.6.1",
    "cross-env": "^5.1.3",
    "css-loader": "^0.28.8",
    "eslint": "^4.15.0",
    "eslint-plugin-babel": "4.1.2",
    "extract-text-webpack-plugin": "3.0.2",
    "favicons-webpack-plugin": "0.0.7",
    "file-loader": "^1.1.6",
    "html-loader": "^0.5.4",
    "html-webpack-plugin": "2.30.1",
    "make-dir": "^1.1.0",
    "make-dir-cli": "^1.0.0",
    "node-sass": "4.7.2",
    "rimraf": "2.6.2",
    "sass-lint": "1.12.1",
    "sass-loader": "6.0.6",
    "static-generator": "^0.2.1",
    "style-loader": "^0.19.1",
    "uglifyjs-webpack-plugin": "^1.1.6",
    "webpack": "^3.10.0",
    "webpack-dev-server": "^2.10.0"
  },
  "dependencies": {
    "chalk": "2.3.0",
    "cors": "2.8.4",
    "express": "4.16.2",
    "key-code": "0.0.1",
    "mathjs": "^3.19.0",
    "merge-options": "1.0.0",
    "normalize.css": "7.0.0",
    "osc-js": "1.2.0",
    "stats.js": "0.17.0",
    "three": "^0.89.0"
  }
}
