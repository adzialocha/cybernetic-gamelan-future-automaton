{
  "name": "cybernetic-gamelan-future-automaton",
  "version": "0.1.0",
  "description": "For laptop ensemble",
  "main": "src/index.html",
  "scripts": {
    "build": "cross-env NODE_ENV=production npm run clean:dist && webpack",
    "clean": "npm run clean:dist && rimraf node_modules",
    "clean:dist": "rimraf dist/",
    "lint": "npm run lint:js && npm run lint:scss",
    "lint:js": "eslint automaton/scripts; exit 0",
    "lint:scss": "sass-lint -c .sass-lint.yml 'automaton/styles/*.scss' -v -q",
    "postinstall": "npm run build",
    "serve": "webpack-dev-server --host 0.0.0.0",
    "serve:express": "cross-env NODE_ENV=development node server/index.js",
    "serve:express:production": "node server/index.js",
    "start": "npm run serve:express:production",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/adzialocha/cybernetic-gamelan-future-automaton.git"
  },
  "author": "adzialocha",
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/adzialocha/cybernetic-gamelan-future-automaton/issues"
  },
  "homepage": "https://github.com/adzialocha/cybernetic-gamelan-future-automaton#readme",
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.1.1",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-preset-es2015": "^6.24.1",
    "cross-env": "^5.0.5",
    "css-loader": "^0.28.4",
    "eslint": "^4.4.1",
    "eslint-plugin-babel": "^4.1.2",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^0.11.2",
    "html-loader": "^0.5.1",
    "html-webpack-plugin": "^2.30.1",
    "node-sass": "^4.5.3",
    "rimraf": "^2.6.1",
    "sass-lint": "^1.10.2",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.18.2",
    "webpack": "^3.5.5",
    "webpack-dev-server": "^2.7.1"
  },
  "dependencies": {
    "express": "^4.15.4",
    "normalize.css": "^7.0.0",
    "peer": "^0.2.8",
    "peerjs": "^0.3.14",
    "three": "^0.87.1",
    "timesync": "^0.4.1"
  }
}
